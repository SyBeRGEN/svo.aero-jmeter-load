              <stringProp name="script">import java.time.*
import java.time.format.DateTimeFormatter
import java.util.Random

def random = new Random()
def formatter = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&quot;)

// –°–ª—É—á–∞–π–Ω–∞—è –¥–∞—Ç–∞: —Å–µ–≥–æ–¥–Ω—è –∏–ª–∏ –∑–∞–≤—Ç—Ä–∞
def baseDate = LocalDate.now()
def randomDayOffset = random.nextInt(2) // 0 –∏–ª–∏ 1
def randomDate = baseDate.plusDays(randomDayOffset)

// –°–ª—É—á–∞–π–Ω–æ–µ –≤—Ä–µ–º—è —Å—Ç–∞—Ä—Ç–∞: –æ—Ç 0:00 –¥–æ 18:00, —à–∞–≥ 30 –º–∏–Ω—É—Ç
def randomHour = random.nextInt(19) // 0‚Äì18, —á—Ç–æ–±—ã —Ö–≤–∞—Ç–∏–ª–æ +6 —á–∞—Å–æ–≤
def randomMinute = random.nextBoolean() ? 0 : 30
def startTime = randomDate.atTime(randomHour, randomMinute)

// –í—Ä–µ–º—è –æ–∫–æ–Ω—á–∞–Ω–∏—è ‚Äî —á–µ—Ä–µ–∑ 6 —á–∞—Å–æ–≤
def endTime = startTime.plusHours(6)

// –ó–∞–ø–∏—Å—å –ø–µ—Ä–µ–º–µ–Ω–Ω—ã—Ö –≤ JMeter
vars.put(&quot;randDateStart&quot;, startTime.format(formatter))
vars.put(&quot;randDateEnd&quot;, endTime.format(formatter))

log.info(&quot;üìÖ –°–ª—É—á–∞–π–Ω—ã–π –∏–Ω—Ç–µ—Ä–≤–∞–ª: ${vars.get(&quot;randDateStart&quot;)} ‚Üí ${vars.get(&quot;randDateEnd&quot;)}&quot;)
</stringProp>
