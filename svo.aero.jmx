              <stringProp name="script">import groovy.json.JsonSlurper

def body = prev.getResponseDataAsString()
def json = new JsonSlurper().parseText(body)

// –ú–Ω–æ–∂–µ—Å—Ç–≤–æ —É–∂–µ –∑–∞–ª–æ–≥–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö —Ä–µ–π—Å–æ–≤
def loggedFlights = new HashSet&lt;String&gt;()

// –ü—É—Ç—å –∫ –ª–æ–≥-—Ñ–∞–π–ª—É
def file = new File(&quot;/home/sybergen/–î–æ–∫—É–º–µ–Ω—Ç—ã/svo.aero/out/results.log&quot;)

def city = vars.get(&quot;city&quot;) ?: &quot;UNKNOWN_CITY&quot;
file.withWriterAppend(&quot;UTF-8&quot;) { writer -&gt;
    writer.writeLine(&quot;===== GET departure by ${city} =====&quot;)
}

json.items.each { item -&gt;
    def flt = item.flt ?: &quot;NO_FLIGHT&quot;
    if (!loggedFlights.contains(flt)) {
        def from    = item.mar1?.city ?: &quot;NO_FROM&quot;
        def to      = item.mar2?.city ?: &quot;NO_TO&quot;
        def status  = item.status_id ?: &quot;NO_STATUS&quot;
        def checkin = item.estimated_chin_start ?: &quot;NO_TIME&quot;
        def logLine = &quot;–ò–∑: ${from} ‚Üí –í: ${to} | –†–µ–π—Å: ${flt} | –ß–µ–∫–∏–Ω–∏–∑–∞—Ü–∏—è: ${checkin} | –°—Ç–∞—Ç—É—Å: ${status}&quot;

        log.info(logLine)
        file.withWriterAppend(&quot;UTF-8&quot;) { writer -&gt;
            writer.writeLine(logLine)
        }

        loggedFlights.add(flt)
    }
}
</stringProp>
